@{
    var form_view_data = (Dictionary<string, string>)ViewBag.form_view_data;

    var value = form_view_data["1"];
}

<div class="form-group">
    @MvcHtmlString.Create(form_view_data["1"])


</div>


<div class="form-group">
    <label>Attachments</label>

    <table class="table table-bordered" id="tb_items">
        <thead>
            <tr>

                <th style="background-color:transparent">File name</th>
                <th style="background-color:transparent">Flie</th>
                <th style="background-color:transparent"><button type="button" class="btn btn-xs btn-primary" onclick="add_row('tb_items')">Add Row</button></th>
            </tr>
        </thead>
        <tbody>
            <tr>

                <td><input class="form-control" required type="text" name="filename" /></td>
                <td><input class="form-control" type="file" name="file" /></td>
                <td><button class="btn btn-xs btn-danger" type="button" onclick="delete_row(this,'tb_items')"><i class="fa fa-trash"></i></button></td>
            </tr>
        </tbody>
    </table>
</div>





<div class="form-group">
    <label>Additional Comments</label>
    <textarea class="form-control" name="comment"></textarea>
</div>
