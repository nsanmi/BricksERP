@using HRM.DAL.Util;
@using HRM.DAL.Models;


@{

    var lookup = (ws_lookup)ViewBag.lookup;

    ViewBag.Title = "lookup";
}



<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Update Job title</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Noticeboard","Admin")">Administration</a>
            </li>
            <li>
                <a>HRM</a>
            </li>
            <li>
                <a>Lookup</a>
            </li>
            <li class="active">
                <strong>Lookup</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content  animated fadeInRight">
    <div class="row">
        <div class="col-sm-12">

            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lookup Details </h5>
                </div>

                <div class="ibox-footer">
                    <div class="row">
                        <div class="col-lg-12 ">

                            <div class="panel-body">
                                <form method="post" action="@Url.Action("UpdateLookup","Settings")" data-parsley-validate class="form-horizontal form-label-left">
                                    <div class="form-group">
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            <label for="note">name</label>
                                            <input type="text" id="name" name="name" class="form-control col-md-7 col-xs-12" value="@lookup.lookup_name" />
                                        </div>
                                        <div class="col-md-6 col-sm-6 col-xs-12" style="padding-bottom:10px;">
                                            <label for="jobtitle">Ordered</label>

                                            <input type="text" id="Ordered" name="Ordered" class="form-control col-md-7 col-xs-12" value="@lookup.orderd">
                                        </div>
                  
                                    </div>

                                </form>
                            </div>
                            <div class="pull-right">
                                <button type="submit" class="btn btn-primary">Update lookup</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>


